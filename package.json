{
  "name": "my-app",
  "version": "0.1.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "ts:check": "tsc --noEmit",
    "db:new-migrate": "prisma migrate dev",
    "db:client": "prisma generate",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "5.5.2",
    "@tanstack/react-query": "^5.8.1",
    "next": "14.0.1",
    "react": "^18",
    "react-dom": "^18",
    "zod": "^3.22.4",
    "zod-prisma-types": "^3.1.0"
  },
  "devDependencies": {
    "@bluzzi/eslint-config": "^1.2.1",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.1",
    "postcss": "^8",
    "prisma": "^5.5.2",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  },
  "eslintConfig": {
    "extends": [
      "@bluzzi",
      "next/core-web-vitals"
    ],
    "rules": {
      "@typescript-eslint/explicit-function-return-type": "off"
    },
    "ignorePatterns": [
      "prisma/generated/*"
    ]
  }
}
